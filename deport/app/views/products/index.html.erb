<head>
  <%= stylesheet_link_tag "index", :media => "all" %>  
</head>
<div id="product_list" >
  <H1>Product List</H1>
  <hr>
  <table>
  <% @products.each do |product| %>
  <tr style="height: 10px;" class="<%= cycle('list_line_odd', 'list_line_even') %>" >
  <td class="img">
  <%= image_tag(product.image_url, :class => 'list_image') %>
  </td>
  <td class="list_description">
  <dl>
  <dt><%= product.title %></dt>
  <dd><%= truncate(strip_tags(product.description),
  :length => 80) %></dd>
  </dl>
  </td>
  <td class="list_actions" >
  <%= link_to 'Show', product %><br/>
  <%= link_to 'Edit', edit_product_path(product) %><br/>
  <%= link_to 'Destroy', product,
  :confirm => 'Are you sure?',
  :method => :delete %>
  </td>
  </tr>
  <% end %>
  </table>
  </div>
  <br />
<div class="new_product"><%= link_to 'New product', new_product_path %></div>

<button>
  <%= link_to("Go to webapp", controller: 'store', action: 'index') %>
</button>


